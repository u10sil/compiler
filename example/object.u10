let model = Model {
	database: MySql {
		connection: Url "mysql://user:password@server.domain.tld/database"
		tables: {
			users: {
				columns: {
					id: { type: "number" indexed: true }
					name: { type: "string" }
				}
			}
		}
	}
	service: Rest {
		listen: Url "https://0.0.0.0:80/api"
		root: Resource {
			users: Collection {
				create: data => data
				read: id => model.database.tables.users.select(id)
				update: (id, data) => false
				delete: id => false
			}
		}
	}
}
